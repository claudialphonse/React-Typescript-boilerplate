{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier":true,
        "member-access": [true, "no-public"],
        "object-literal-sort-keys": [false, "ignore-case"],
        "no-console": false
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}